{
  "user": {
    "name": "vagrant",
    "group": "contentsuser"
  },
  "php": {
    "app_name": "php",
    "fqdn": "php.takkyuuplayer.vagrant",
    "ssl": {
      "ssl_certificate": "/etc/ssl/private/server.crt",
      "ssl_certificate_key": "/etc/ssl/private/server.key"
    }
  },
  "proxy": {
    "app_name": "proxy",
    "fqdn": "proxy.takkyuuplayer.vagrant",
    "port": 8000
  },
  "nginx": {
    "ssl": {
      "ssl_certificate": "/etc/ssl/private/server.crt",
      "ssl_certificate_key": "/etc/ssl/private/server.key"
    }
  }
}
