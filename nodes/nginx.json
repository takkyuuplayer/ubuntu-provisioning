{
  "user": {
    "name": "vagrant",
    "group": "contentsuser",
    "linger": true
  },
  "nginx": {
    "ssl": {
      "crt": "/etc/ssl/private/vagrant.crt",
      "key": "/etc/ssl/private/vagrant.key"
    },
    "virtual_hosts": [
      {
        "app_name": "proxy",
        "template": "proxy",
        "fqdn": "proxy.takkyuuplayer.vagrant",
        "port": 4567,
        "ssl": {
          "crt": "/etc/ssl/private/vagrant.crt",
          "key": "/etc/ssl/private/vagrant.key"
        }
      },
      {
        "app_name": "php",
        "template": "php",
        "fqdn": "php.takkyuuplayer.vagrant",
        "ssl": {
          "crt": "/etc/ssl/private/vagrant.crt",
          "key": "/etc/ssl/private/vagrant.key"
        }
      },
      {
        "app_name": "spa",
        "template": "spa",
        "fqdn": "spa.takkyuuplayer.vagrant",
        "port": 5000,
        "ssl": {
          "crt": "/etc/ssl/private/vagrant.crt",
          "key": "/etc/ssl/private/vagrant.key"
        }
      }
    ]
  }
}
