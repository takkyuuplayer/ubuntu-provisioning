{
  "user": {
    "name": "vagrant",
    "group": "contentsuser",
    "linger": true
  },
  "nginx": {
    "ssl": {
      "crt": "/etc/ssl/private/vagrant.crt",
      "key": "/etc/ssl/private/vagrant.key"
    },
    "virtual_hosts": [
      {
        "app_name": "homepage",
        "fqdn": "homepage.takkyuuplayer.vagrant",
        "port": 4567,
        "template": "proxy",
        "ssl": {
          "crt_path": "/etc/ssl/private/vagrant.crt",
          "key_path": "/etc/ssl/private/vagrant.key"
        }
      }
    ]
  }
}
