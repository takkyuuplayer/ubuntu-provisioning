{
  "user": {
    "name": "takkyuuplayer",
    "group": "contentsuser"
  },
  "anyenv": {
    "anyenv_root": "/usr/local/anyenv",
    "anyenvrc_file": "/usr/local/etc/anyenvrc",
    "install_versions": {
      "rbenv": ["2.3.0"]
    }
  },
  "proxy": {
    "app_name": "homepage",
    "fqdn": "www.takkyuuplayer.com",
    "port": 4567,
    "ssl": {
      "ssl_certificate": "/etc/letsencrypt/live/www.takkyuuplayer.com/fullchain.pem",
      "ssl_certificate_key": "/etc/letsencrypt/live/www.takkyuuplayer.com/privkey.pem"
    }
  },
  "supervisor": {
    "app_name": "homepage",
    "command": "/usr/local/anyenv/envs/rbenv/shims/bundle exec -- unicorn -c unicorn.rb -E production",
    "directory": "/workspace/deployment/homepage/current"
  }
}
