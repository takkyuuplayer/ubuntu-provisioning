{
  "user": {
    "name": "takkyuuplayer",
    "group": "contentsuser",
    "linger": true
  },
  "anyenv": {
    "anyenv_root": "/usr/local/anyenv",
    "anyenvrc_file": "/usr/local/etc/anyenvrc",
    "install_versions": {
      "rbenv": [
        "2.3.0"
      ]
    }
  },
  "proxy": {
    "app_name": "homepage",
    "fqdn": "www.takkyuuplayer.com",
    "port": 4567,
    "ssl": {
      "ssl_certificate": "/etc/letsencrypt/live/www.takkyuuplayer.com/fullchain.pem",
      "ssl_certificate_key": "/etc/letsencrypt/live/www.takkyuuplayer.com/privkey.pem"
    }
  }
}
