{
  "user": {
    "name": "takkyuuplayer",
    "group": "contentsuser",
    "linger": true
  },
  "crontab": {
    "email": "takkyuuplayer@gmail.com"
  },
  "anyenv": {
    "anyenv_root": "/usr/local/anyenv",
    "anyenvrc_file": "/usr/local/etc/anyenvrc",
    "install_versions": {
      "rbenv": [
        "2.3.0"
      ]
    }
  },
  "tmux": {
    "version": "2.5"
  },
  "nginx": {
    "virtual_hosts": [
      {
        "app_name": "homepage",
        "fqdn": "www.takkyuuplayer.com",
        "port": 4567,
        "template": "proxy",
        "ssl": {
          "crt": "/etc/letsencrypt/live/www.takkyuuplayer.com/fullchain.pem",
          "key": "/etc/letsencrypt/live/www.takkyuuplayer.com/privkey.pem"
        }
      }
    ],
    "log_archive": {
      "aws_profile": "s3-writer",
      "s3_bucket": "tp-nginx-log-archive"
    }
  },
  "monitor": {
    "mackerel": {
      "apikey": false
    }
  },
  "letsencrypt": {
    "email": "takkyuuplayer@gmail.com"
  }
}
